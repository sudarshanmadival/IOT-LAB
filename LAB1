//ODD EVEN LED
const int led[]={2,3,4,5,6};
const int nled=5;
int i;

void setup()
{
  for(i=0;i<nled;i++){
    pinMode(led[i], OUTPUT);}
}

void loop()
{
  for(i=1;i<nled;i+=2)
  {
    
  digitalWrite(led[i], HIGH);
  delay(100); 
  digitalWrite(led[i], LOW);
  delay(100);
  }
  
 for(i=0;i<nled;i+=2)
  {
  digitalWrite(led[i], HIGH);
  delay(100);
  digitalWrite(led[i], LOW);
  delay(100);}
  }
